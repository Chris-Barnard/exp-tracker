<!DOCTYPE html>
<html ng-app="app">
	<head>
    	<title><%= title %></title>
    	<!-- inject-vendor:css -->
		<link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.min.css" />
		<link rel="stylesheet" type="text/css" href="/stylesheets/ng-tags-input.css" />
		<link rel="stylesheet" type="text/css" href="/stylesheets/ng-tags-input.bootstrap.css" />
		<!-- endinject -->

		<!-- inject:css -->
    	<link rel='stylesheet' href='/stylesheets/style.css' />
    	<!-- endinject -->
	</head>
	<body>
		<header class="page-header">
			<h1 class="text-center"><%= title %></h1>
		</header>
		<div>
			<div ng-include="'app/layout/shell.html'"></div>
		</div>
		<!-- inject-vendor:js -->
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
		<script src="/javascripts/bootstrap.min.js"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
		<script type="text/javascript" src="/javascripts/moment.js"></script>
		<script type="text/javascript" src="/javascripts/angular-moment.js"></script>
		<script type="text/javascript" src="/javascripts/ng-tags-input.js"></script>
		<script type="text/javascript" src="/javascripts/angular-tablesort.js"></script>
		<script type="text/javascript" src="/javascripts/angular-modal-service.js"></script>
		<script type="text/javascript" src="/javascripts/ui-bootstrap-custom-0.12.1.js"></script>
		<script type="text/javascript" src="/javascripts/ui-bootstrap-custom-tpls-0.12.1.js"></script>
		<!-- endinject -->

		<!-- inject:js -->
		<!-- bootstrapping -->
		<script type="text/javascript" src="/app/app.module.js"></script>

		<!-- core module -->
		<script type="text/javascript" src="/app/core/core.module.js"></script>
		<script type="text/javascript" src="/app/core/dataservice.js"></script>

		<!-- modals module -->
		<script type="text/javascript" src="/app/modals/modals.module.js"></script>
		<script type="text/javascript" src="/app/modals/expEditModal.js"></script>

		<!-- layout module -->
		<script type="text/javascript" src="/app/layout/layout.module.js"></script>
		<script type="text/javascript" src="/app/layout/shell.js"></script>

		<!-- expenses module -->
		<script type="text/javascript" src="/app/expenses/expenses.module.js"></script>
		<script type="text/javascript" src="/app/expenses/addExpenses.directive.js"></script>
		<script type="text/javascript" src="/app/expenses/reviewMonthlyExpenses.directive.js"></script>
		<script type="text/javascript" src="/app/expenses/reviewTaggedExpenses.directive.js"></script>

		<!-- endinject -->
	</body>
</html>
